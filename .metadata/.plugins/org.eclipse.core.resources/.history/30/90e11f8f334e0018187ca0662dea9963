import java.awt.Color;
import java.awt.Graphics;

public class Test extends Ammo
{
	public Test()
	{
		this(0,0,0,0);
	}

	public Test(int x, int y)
	{
		//add code
		this(x,y,0,0);
	}

	public Test(int x, int y, int s, int d)
	{
		//add code
		super(x,y,10,10,s,d);

	}

	public Test(int x, int y, int w, int h, int s, int d) {
		super(x,y,w,h,s,d);

	}

	public void draw( Graphics window )
	{
		for (int i=0; i<getX()/2; i++) {
			window.setColor(new Color(255,(int)Math.abs(255*Math.sin((1.0*i/getWidth())*(Math.PI/2))),0));
			window.fillOval(getX()+i, getY()+i, getWidth()-2*i, getHeight()-2*i);	
		}
	}
	

	public String toString()
	{
		return super.toString();
	}
}
