import java.io.File;
import java.net.URL;
import java.awt.Color;
import java.awt.Graphics;
import java.awt.Image;
import javax.imageio.ImageIO;

public class Alien1b extends Alien1
{

	public Alien1b()
	{
		this(0,0,30,30,0,1);
	}

	public Alien1b(int x, int y, int w, int h, int s, int l)
	{
		super(x, y, w, h, s, l);
	}
	
	public void set(int x, int y, int w, int h, int s, int l) {
		super.set(x, y, w, h, s, l);
	}

    public void move()
	{
    	
    	if ((getY()%60<11||(getY()%60<31&&getY()%60>20))&&getX()+getWidth()+getSpeed()<=783) {
    		for (int i=0; i<getSpeed(); i++) {
        		super.move("RIGHT");
        	}
    		super.move("DOWN");
		} else if (getY()%60<21&&getX()+getWidth()+getSpeed()<=783) {
    		for (int i=0; i<2*getSpeed(); i++) {
        		super.move("RIGHT");
        	}
    		super.move("DOWN");
		} else if (getY()%60<41&&getX()-getSpeed()>0){
			super.move("LEFT");
		} else {
			super.move("DOWN");
		}
    	
    	System.out.println("hi");
	}
   
	public String toString()
	{
		return "";
	}
}
