import java.awt.Color;
import java.awt.Graphics;

public class Test extends Ammo
{
	public Test()
	{
		this(0,0,0,0);
	}

	public Test(int x, int y)
	{
		//add code
		this(x,y,0,0);
	}

	public Test(int x, int y, int s, int d)
	{
		//add code
		super(x,y,10,10,s,d);

	}

	public Test(int x, int y, int w, int h, int s, int d) {
		super(x,y,w,h,s,d);

	}

	public void draw( Graphics window )
	{
		window.setColor(new Color(255,0,0));
		for (int i=0; i<getX(); i++) {
			window.setColor(new Color(255,5*i,0));
			window.fillRect(getX()-i, getY()-i, getWidth(), getHeight());	
		}
	}
	

	public String toString()
	{
		return super.toString();
	}
}
